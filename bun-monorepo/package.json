{
  "catalog": {
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/vite": "4.1.17",
    "@tanstack/react-query": "5.90.21",
    "@types/bun": "1.3.9",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "babel-plugin-react-compiler": "1.0.0",
    "globals": "16.5.0",
    "hono": "4.8.3",
    "jsdom": "27.2.0",
    "lucide-react": "0.555.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.6",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "vite": "7.2.4",
    "vite-bundle-analyzer": "1.2.3",
    "vitest": "4.0.14",
    "zod": "4.1.13"
  },
  "description": "Bun monorepo template with Turbo, Hono API, Biome, and Docker support",
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@commitlint/types": "20.2.0",
    "@cspell/dict-es-es": "3.0.8",
    "@types/bun": "catalog:",
    "@types/node": "catalog:",
    "cspell": "9.4.0",
    "editorconfig-checker": "6.1.1",
    "lefthook": "2.0.12",
    "syncpack": "14.0.0-alpha.32",
    "turbo": "2.6.3",
    "vitest": "catalog:"
  },
  "engines": {
    "bun": ">=1.3.9",
    "node": ">=24"
  },
  "name": "bun-monorepo",
  "packageManager": "bun@1.3.9",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "ci": "turbo run build check-types test && bun run lint",
    "clean": "find . -type d \\( -name 'node_modules' -o -name 'build' -o -name 'dist' -o -name '.turbo' -o -name '.expo' -o -name 'android' -o -name 'ios' -o -name '.astro' \\) -prune -exec rm -rf '{}' +",
    "dev": "turbo run dev",
    "docker:build": "turbo run docker:build",
    "docker:run": "turbo run docker:run",
    "lint": "biome check --error-on-warnings",
    "lint:fix": "biome check --error-on-warnings --write",
    "preinstall": "npx only-allow bun",
    "prepare": "lefthook install",
    "spell-check": "cspell -c ./cspell.config.mts .",
    "test": "vitest run",
    "test:e2e": "true # turbo run test:e2e",
    "test:integration": "true # vitest run --config=vitest.integration.config.ts",
    "test:integration:watch": "vitest --watch --config=vitest.integration.config.ts",
    "test:watch": "vitest --watch"
  },
  "trustedDependencies": ["editorconfig-checker"],
  "type": "module",
  "version": "0.0.0",
  "workspaces": ["apps/*", "packages/*", "turbo/*"]
}
