{
  "bin": {
    "bun-library-solana-kit": "dist/cli.js"
  },
  "dependencies": {
    "@solana/kit": "^6.0.1",
    "@solana/kit-plugin-rpc": "^0.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@changesets/cli": "^2.29.8",
    "@types/bun": "latest",
    "pkg-pr-new": "^0.0.62",
    "publint": "^0.3.16",
    "tsup": "^8.5.1"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "name": "bun-library-solana-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beeman/bun-library-solana-kit.git"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "tsup",
    "changeset": "changeset",
    "check-types": "tsc -b --noEmit",
    "lint": "biome check --error-on-warnings",
    "postinstall": "[ -f .env.example ] && [ ! -f .env ] && cp -v .env.example .env || true",
    "lint:fix": "biome check --error-on-warnings --write",
    "release": "changeset publish",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "version": "changeset version && bun lint:fix"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}
