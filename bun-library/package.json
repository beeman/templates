{
  "bin": {
    "bun-library": "dist/cli.mjs"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "^2.4.0",
    "@changesets/cli": "^2.29.8",
    "@intellectronica/ruler": "^0.3.34",
    "@types/bun": "^1.3.9",
    "pkg-pr-new": "^0.0.62",
    "publint": "^0.3.16",
    "tsdown": "^0.20.3"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "name": "bun-library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beeman/bun-library.git"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "tsdown",
    "changeset": "changeset",
    "check-types": "tsc -b --noEmit",
    "ci": "bun run build && bun run lint && bun run check-types && bun test",
    "lint": "biome check --error-on-warnings",
    "lint:fix": "biome check --error-on-warnings --write",
    "release": "changeset publish",
    "ruler:apply": "ruler apply --local-only",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "version": "changeset version && bun lint:fix"
  },
  "type": "module",
  "types": "./dist/index.d.mts",
  "version": "0.0.0",
  "description": "TypeScript library with Bun, tsdown, Biome, and Changesets"
}
